{
    "family": "baseball-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "baseball-container",
            "image": "709205728612.dkr.ecr.us-east-1.amazonaws.com/livebaseballscorecards:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "protocol": "tcp",
                    "hostPort": 80
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256", 
    "memory": "512"
}
